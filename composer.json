{
  "name": "builderius\/swoop",
  "description": "OOP based platform for Wordpress plugins creation",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Swoop\\Bundle\\": "src/Bundle/"
    }
  },
  "require": {
    "php": ">=8.2",
    "symfony/config": "^7.0",
    "symfony/event-dispatcher": "^7.0",
    "symfony/finder": "^7.0",
    "symfony/filesystem": "^7.0",
    "symfony/dependency-injection": "^7.0",
    "symfony/yaml": "^7.0",
    "symfony/twig-bridge": "^7.0",
    "dg/composer-cleaner": "^2.2"
  },
  "config": {
    "vendor-dir": "vendor-prefixed",
    "allow-plugins": {
      "dg/composer-cleaner": true
    }
  }
}
